<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-Bus Object Introspection 1.0//EN"
  "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.bluez.GattService1">

    <!-- 128-bit UUID of the service -->
    <property name="UUID" type="s" access="read"/>

    <!-- Indicates if this is a primary service -->
    <property name="Primary" type="b" access="read"/>

    <!-- Object paths of included services -->
    <property name="Includes" type="ao" access="read"/>

    <!-- Handle to be used when registering this service (0x0000 means auto allocate) -->
    <property name="Handle" type="q" access="readwrite"/>

  </interface>
</node>
